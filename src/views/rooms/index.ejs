<!DOCTYPE html>
<html>
  <head>
    <% include ../static/partials/head.ejs %>
  </head>
  <body>
    <main class="container">
      <% include ../static/partials/navbar.ejs %>

      <h1>Rooms</h1>

      <ul class="list-group">

        <% rooms.forEach((room) => { %>

          <li class="list-group-item">
            <a href="/rooms/<%= room.id %>"> <%= room.title %> </a>
          </li>
        <% }) %>
      </ul>

    </main>
  </body>
</html>
